<head>
    <link rel="stylesheet" href="/stylesheet/registracion.css">
    <title><%= title %></title> 
</head>
<header>
    <%-include('./partials/header')%>
 </header>
<main>
    <div class="uk-section uk-section-small">
        <div class="uk-container uk-container-large uk-flex uk-flex-middle uk-flex-center">
            <form action="">
                
                <h2>Formulario de registración</h2>
                <label for="nombre">Nombre:</label><br>
                <input type="text" name="nombre" id="nombre" placeholder="Lautaro" required="required"><br>
                <label for="apellido">Apellido:</label><br>
                <input type="text" name="apellido" id="apellido" placeholder="Ortiz" required="required"><br>
                <label for="apodo">Apodo:</label><br>
                <input type="text" name="apodo" id="apodo" placeholder="Lauti"><br>
                <form>
            
                    <label for="genero">Género:</label><br>
                    <input type="radio" id="hombre" name="genero" value="hombre">
                    <label for="hombre">Hombre</label><br>
                    <input type="radio" id="mujer" name="genero" value="mujer">
                    <label for="mujer">Mujer</label><br>
                    <input type="radio" id="otro" name="genero" value="otro">
                    <label for="otro">Otro</label><br>

                    <label for="nacimiento">Fecha de nacimiento:</label><br>
                    <input type="date" required="required"><br>
                    <br><input type="submit">
                  </form>
            </form>
        </div>
    </div>
</main>
 <footer>
    <%-include('./partials/footer')%>
</footer>